<?xml version="1.0"?>
<info>
    <id>tokenbasedav</id>
    <name>Token base DAV</name>
    <description>enabling token acces on DAV file sharing</description>
    <licence>AGPL</licence>
    <author>Ponder source</author>
    <version>0.0.2</version>
    <namespace>TokenBaseDav</namespace>
    <website>https://github.com/pondersource/dav-token-access</website>
	<bugs>https://github.com/pondersource/dav-token-access/issues</bugs>
	<repository type="git">https://github.com/pondersource/dav-token-access.git</repository>
    <category>tool</category>
    <dependencies>
        <owncloud min-version="10" max-version="10" />
    </dependencies>
    <types>
        <authentication/>
        <dav/>
    </types>

    <sabre>
		<plugins>
			<plugin>OCA\TokenBaseDav\DAVPlugin\TokenAuth</plugin>
		</plugins>
	</sabre>
</info>